#include<iostream>
#include<stdio.h>
#include<cstring>
#include<cstdlib>
#include<algorithm>
#define LL long long
using namespace std;
 
const int N = 50086;
 
int main()
{
    char a[100][100];
    for(int i = 0; i < 100; i++)
    for(int j = 0; j < 100; j++) a[i][j] = '@';
    for(int i = 0; i < 100; i++)
    {
        for(int j = 0; i < 100; j++)
        {
            cout<<a[i][j];
        }
    }
 
    return 0;
}
